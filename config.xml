<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.myapp804719" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>hackafe-mobile</name>
  <description>
        Hackafe Mobile App
    </description>
  <author email="info@hackafe.org" href="http://hackafe.org/">
      Hackafe
    </author>
  <content src="index.html"/>
  <access origin="*"/>
